                <Label Content="雷达版本号:" FontSize="16"  Foreground="#FFFFFF"/>
                <TextBlock Text="{Binding Version}" FontSize="16"  Foreground="#FFFFFF" MouseDown="VersionTextBox_MouseDown" Margin="0 5 0 0"/>
            </StackPanel>
        </Grid>
    </Grid>
</MetroControl:MetroWindow>
