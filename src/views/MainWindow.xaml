                            </LiveChart:VisualElement.UIElement>
                        </LiveChart:VisualElement>
                        <LiveChart:VisualElement X="0.3" Y="0.3" Width="1" Height="1">
                            <LiveChart:VisualElement.UIElement>
                                <Image  Source="/DAHUA_ITS_A08;component/image/mbit.ico"/>
                            </LiveChart:VisualElement.UIElement>
                        </LiveChart:VisualElement>
