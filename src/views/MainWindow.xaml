            <StackPanel DockPanel.Dock="Left" Orientation="Horizontal" Grid.Column="0">
                <Label Content="雷达连接状态:" FontSize="16"  Foreground="#FFFFFF" Width="110" Height="30"/>
                <TextBlock Text="{Binding ConnectState, Converter={StaticResource ConnectState2String}}" FontSize="16"  Foreground="#FFFFFF" VerticalAlignment="Center"/>
            </StackPanel>
            <StackPanel DockPanel.Dock="Right" Orientation="Horizontal" Grid.Column="1">
