                        <Button Content="{StaticResource GetStrongestPoint}" Width="60"  Style="{StaticResource Style_Button_Ok}" Command="{Binding GetStrongestPointCmd}" Margin="5 0" ToolTip="{StaticResource FuncGetStrongestPoint}"/>
                        <Button Content="{StaticResource CancelGet}" Width="60"  Style="{StaticResource Style_Button_Ok}" Command="{Binding CancelGetCmd}" Margin="0 0"  ToolTip="{StaticResource FuncCancelGet}"/>
                    </StackPanel>
                </StackPanel>
            </Border>
        </Grid>
        <Grid Background="#202020" Grid.Row="3">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100*"/>
                <ColumnDefinition Width="100*"/>
